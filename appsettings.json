{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=localhost;Database=umbraco-boilerplate;Integrated Security=true"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "Umbraco.PublishedContent.Models",
        "ModelsDirectory": "~/Models"
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Global": {
        "Id": "889a27c3-de13-4037-ab3e-2f146266d006"
      }
    }
  },
  "uSync": {
    "Settings": {
      "ExportOnSave": "Settings",
      "UIEnabledGroups": "Settings"
    }
  }
}